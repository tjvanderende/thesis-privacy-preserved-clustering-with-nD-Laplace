@article{riosUnifyingFrameworkSystematic2022,
  title = {A Unifying Framework for the Systematic Analysis of {{Git}} Workflows},
  author = {R{\'i}os, Julio C{\'e}sar Cort{\'e}s and Embury, Suzanne M. and Eraslan, Sukru},
  year = {2022},
