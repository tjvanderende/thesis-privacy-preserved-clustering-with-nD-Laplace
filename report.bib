@article{sidey-gibbons_machine_2019,
  title = {Machine Learning in Medicine: A Practical Introduction},
  shorttitle = {Machine Learning in Medicine},
  author = {{Sidey-Gibbons}, Jenni A. M. and {Sidey-Gibbons}, Chris J.},
