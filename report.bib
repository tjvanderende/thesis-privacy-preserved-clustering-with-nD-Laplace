@inproceedings{bassily_local_2015,
  title = {Local, {{Private}}, {{Efficient Protocols}} for {{Succinct Histograms}}},
  booktitle = {Proceedings of the Forty-Seventh Annual {{ACM}} Symposium on {{Theory}} of {{Computing}}},
